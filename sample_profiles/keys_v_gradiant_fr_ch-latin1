# Sample profile vertical gradiant with vars and keys

# Vars :
var raw0 00ffff
var raw1 960096
var raw2 600096
var raw3 300096
var raw4 000096
var raw5 003096
var raw6 006096


# Raw 0 :
g logo $raw0
g indicators $raw0
k mute $raw0

# Raw 1 :
k escape $raw1
g fkeys $raw1
k print_screen $raw1
k scroll_lock $raw1
k pause_break $raw1
k play_pause $raw1
k stop $raw1
k prev $raw1
k next $raw1

# Raw 2 :
k tilde $raw2
k 1 $raw2
k 2 $raw2
k 3 $raw2
k 4 $raw2
k 5 $raw2
k 6 $raw2
k 7 $raw2
k 8 $raw2
k 9 $raw2
k 0 $raw2
k minus $raw2
k equal $raw2
k backspace $raw2
k insert $raw2
k home $raw2
k page_up $raw2
k num_lock $raw2
k num_slash $raw2
k num_asterisk $raw2
k num_minus $raw2

# Raw 3 :
k tab $raw3
k q $raw3
k w $raw3
k e $raw3
k r $raw3
k t $raw3
k y $raw3
k u $raw3
k i $raw3
k o $raw3
k p $raw3
k open_bracket $raw3
k close_bracket $raw3
# k backslash $raw3
k delete $raw3
k end $raw3
k page_down $raw3
k num7 $raw3
k num8 $raw3
k num9 $raw3

# Raw 4 :
k caps_lock $raw4
k a $raw4
k s $raw4
k d $raw4
k f $raw4
k g $raw4
k h $raw4
k j $raw4
k k $raw4
k l $raw4
k semicolon $raw4
k quote $raw4
k dollar $raw4
k enter $raw4
k num4 $raw4
k num5 $raw4
